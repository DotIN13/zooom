{
  "name": "zooom",
  "version": "1.0.0",
  "description": "Stimulus powered image zooming, inspired by zooming.js",
  "main": "index.js",
  "author": "DotIN13",
  "license": "MIT",
  "packageManager": "yarn@3.3.0",
  "scripts": {
    "build:dev": "esbuild index.js --bundle --format=esm > dist/zooom.js",
    "build:prod": "esbuild index.js --bundle --minify --sourcemap --target=chrome79,firefox106,safari12 --format=esm > dist/zooom.min.js",
    "start": "yarn http-server"
  },
  "dependencies": {
    "stimulus": "^3.2.1"
  },
  "devDependencies": {
    "http-server": "^14.1.1"
  }
}
